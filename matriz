#include <stdio.h>

int main(void) {
  int num [3][3];
  int i=0,j=0;
  for (i=0;i<=2;i++){
    printf("Digite o valor da posição ");
        for(j=0;j<=2;j++){
      scanf("%d",& num[i][j]);
    }
  }
  for(i=0;i<=2;i++){
    for(j=0;j<=2;j++){
      printf("%d",num [i][j]);
    }
    printf("\n");
  }
  return 0;
}
